class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int single,i;
        i=1;
        single=nums[0];
        for(;i<nums.size();i++) single^=nums[i];
        return single;
    }
};

#pay attention to the bit manipulation
#another way to solve this problem is to use hash table 
#but i haven't tried that.[if u r interested ]
